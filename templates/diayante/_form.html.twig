<div class="container col-md-9">
    <div class="card">
        <div class="card-header">Formulaire des diayantes</div>
        <div class="card-body">
            {{ form_start(form) }}

            <div class="row  ">
                <div class="col md-4"> {{ form_row(form.montant) }}</div>
            </div>
            <div class="row  ">
                <div class="col md-4"> {{ form_row(form.date) }}</div>
            </div>
            <div class="row  ">
                <div class="col md-4"> {{ form_row(form.eveneement) }}</div>
            </div>
            <div class="row  ">
                <div class="col md-4"> {{ form_row(form.caisse) }}</div>
            </div>

            <br>
            <button class="btn btn-success">{{ button_label|default('enrigistre') }}</button>

        </div>

        {{ form_widget(form) }}
        {{ form_end(form) }}

    </div>
</div>
</div>
