<div class="container col-md-9">
<div class="card">
    <div class="card-header">Formulaire d'inscription</div>
    <div class="card-body">
        {{ form_start(form) }}
        <div class="row  ">
            <div class="col md-4"> {{ form_row(form.nom) }}</div>
        </div>
        <div class="row  ">
            <div class="col md-4"> {{ form_row(form.prenom) }}</div>
        </div>
        <div class="row  ">
            <div class="col md-4"> {{ form_row(form.adresse) }}</div>
        </div>
        <div class="row  ">
            <div class="col md-4"> {{ form_row(form.telephone) }}</div>
        </div>
        <div class="row  ">
            <div class="col md-4"> {{ form_row(form.age) }}</div>
        </div>
        <div class="row  ">
            <div class="col md-4"> {{ form_row(form.sexe) }}</div>
        </div>
        <div class="row  ">
            <div class="col md-4"> {{ form_row(form.commission) }}</div>
        </div>
        <div class="row  ">
            <div class="col md-4"> {{ form_row(form.kourel) }}</div>
        </div>
        <br>
        <button class="btn btn-success">{{ button_label|default('enrigistre') }}</button>

    </div>

        {{ form_widget(form) }}
        {{ form_end(form) }}

    </div>
</div>
</div>
